###  http://lod.tuwien.ac.at/organization/vocab#employmentRole

:employmentRole rdf:type owl:ObjectProperty ;
                
                rdfs:label "anstellungsrolle"@de ,
                           "employment role"@en ;
                
                rdfs:comment "Beschreibt welche Anstellunsgrolle (z.B.: Dozent, Tutor) der Agent in der gegebene Mitgliedschaft an einer Universität spielt."@en ,
                             "Indicates the employment role (e.g. tutor, docent, ...) of the agent of a membership in an university."@en ;
                
                rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
                
                rdfs:domain :Employment ;
                
                rdfs:range :EmploymentRole ;
                
                rdfs:subPropertyOf :role .



###  http://lod.tuwien.ac.at/organization/vocab#hasDeanary

:hasDeanary rdf:type owl:FunctionalProperty ,
                     owl:IrreflexiveProperty ,
                     owl:ObjectProperty ;
            
            rdfs:label "hat Dekanat"@de ,
                       "has deanary"@en ;
            
            rdfs:comment "Diese Fakultät hat das gegebene Dekanat. Einer Fakultät kann nur ein Dekanat zugeordnet werden."@de ,
                         "This faculty has the given deanary. A faculty can have only one deanary."@en ;
            
            rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
            
            rdfs:range :Deanery ;
            
            rdfs:subPropertyOf :hasUnit ;
            
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Department
                                        :Faculty
                                      )
                        ] .



###  http://lod.tuwien.ac.at/organization/vocab#hasInstitute

:hasInstitute rdf:type owl:ObjectProperty ;
              
              rdfs:label "hat Institut"@de ,
                         "has institute"@en ;
              
              rdfs:comment "Diese Fakultät hat das gegebene Institut."@de ,
                           "This faculty has the given institute."@en ;
              
              rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
              
              rdfs:domain :Faculty ;
              
              rdfs:range :Institute ;
              
              rdfs:subPropertyOf :hasUnit .



###  http://lod.tuwien.ac.at/organization/vocab#hasSubOrganization

:hasSubOrganization rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "hat universitäre Suborganisation"@de ,
                               "has university suborganization"@en ;
                    
                    rdfs:isDefinedBy "Die gegebene Universitätsorganisation ist eine Suborganisation dieser Universitätsorganisation."@de ;
                    
                    rdfs:comment "The given university organization is a suborganization of this university organization."@en ;
                    
                    rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
                    
                    rdfs:domain :UniversityOrganization ;
                    
                    rdfs:range :UniversityOrganization ;
                    
                    rdfs:subPropertyOf org:hasSubOrganization .



###  http://lod.tuwien.ac.at/organization/vocab#hasUnit

:hasUnit rdf:type owl:ObjectProperty ;
         
         rdfs:label "hat Organisationseinheit"@de ,
                    "has organizational unit"@en ;
         
         rdfs:commenet "Diese Universitätsorganisation hat die gegebene universitäre Organisationseinheit als Suborganisaiton."@de ,
                       "This university organization has the given university organizational unit as suborganization."@en ;
         
         rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
         
         rdfs:domain :UniversityOrganization ;
         
         rdfs:range :UniversityOrganizationalUnit ;
         
         rdfs:subPropertyOf :hasSubOrganization .



###  http://lod.tuwien.ac.at/organization/vocab#isDeanaryOf

:isDeanaryOf rdf:type owl:FunctionalProperty ,
                      owl:IrreflexiveProperty ,
                      owl:ObjectProperty ;
             
             rdfs:label "ist Dekanat von"@de ,
                        "is deanary of"@en ;
             
             rdfs:comment "Diese Universitätsorganisation ist das Dekanat der gegebenen Fakultät oder Abteilung."@de ,
                          "This university organization is the deanary of the given faculty or division."@en ;
             
             rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
             
             rdfs:domain :Deanery ;
             
             owl:inverseOf :hasDeanary ;
             
             rdfs:subPropertyOf :unitOf ;
             
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Department
                                        :Faculty
                                      )
                        ] .



###  http://lod.tuwien.ac.at/organization/vocab#isInstituteOf

:isInstituteOf rdf:type owl:ObjectProperty ;
               
               rdfs:label "ist Institut von"@de ,
                          "is institute of"@en ;
               
               rdfs:comment "Diese Universitätsorganisation ist ein Institut der gegebenen Universitätsorganisation."@en ,
                            "This university organization is the institute of the given faculty."@en ;
               
               rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
               
               rdfs:range :Faculty ;
               
               rdfs:domain :Institute ;
               
               owl:inverseOf :hasInstitute ;
               
               rdfs:subPropertyOf :unitOf .



###  http://lod.tuwien.ac.at/organization/vocab#role

:role rdf:type owl:ObjectProperty ;
      
      rdfs:label "rolle"@de ,
                 "role"@en ;
      
      rdfs:comment "Beschreibt welche Universitätstrolle (z.B.: Student, Vortragender,...) der Agent in der gegebene Mitgliedschaft an einer Universität spielt."@de ,
                   "Indicates the university role (e.g. student, lecturer, ...) of the agent of a membership in an university."@en ;
      
      rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
      
      rdfs:domain :UniversityMembership ;
      
      rdfs:range :UniversityRole ;
      
      rdfs:subPropertyOf org:role .



###  http://lod.tuwien.ac.at/organization/vocab#subOrganizationOf

:subOrganizationOf rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "ist Suborganisation von"@de ,
                              "is suborganization of"@en ;
                   
                   rdfs:comment "Diese Universitätsorganisation ist eine Suborganisation der gegebenen Universitätsorganisation."@de ,
                                "This university organization is a suborganization of the given organization."@en ;
                   
                   rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
                   
                   rdfs:domain :UniversityOrganization ;
                   
                   rdfs:range :UniversityOrganization ;
                   
                   owl:inverseOf :hasSubOrganization ;
                   
                   rdfs:subPropertyOf org:subOrganizationOf .



###  http://lod.tuwien.ac.at/organization/vocab#unitOf

:unitOf rdf:type owl:ObjectProperty ;
        
        rdfs:label "ist Organisationseinheit von"@de ,
                   "is organizational unit of"@en ;
        
        rdfs:comment "Diese Organisationseinheit einer Universität ist die Einheit einer gegebenen Universitätsorganisation."@de ,
                     "This university organizational unit is the unit of the given university organization."@en ;
        
        rdfs:isDefinedBy <http://lod.tuwien.ac.at/organization/vocab#> ;
        
        rdfs:range :UniversityOrganization ;
        
        rdfs:domain :UniversityOrganizationalUnit ;
        
        owl:inverseOf :hasUnit ;
        
        rdfs:subPropertyOf :subOrganizationOf .





